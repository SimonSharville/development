<!-- SCRIPTS ============================================================================ -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>

    <!-- https://teamtreehouse.com/community/random-background-color -->
    <script type="text/javascript">
    function ran_col() { //function name
        var color = '#'; // hexadecimal starting symbol
        var letters = ['2F2B2D','728C8C','4819A9','0D8078','0D8078','96C514','E8A800','A4197E','452597','8A3E04']; //Set your colors here
        color += letters[Math.floor(Math.random() * letters.length)];
        document.getElementById('indexPage').style.background = color; // Setting the random color on your div element.
    }
</script>

